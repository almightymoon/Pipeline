{
  "dashboard": {
    "id": null,
    "title": "Pipeline Dashboard",
    "tags": ["pipeline"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Pipeline Status",
        "type": "stat",
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "3",
            "legendFormat": "Total Runs",
            "refId": "A"
          },
          {
            "expr": "2",
            "legendFormat": "Successful",
            "refId": "B"
          },
          {
            "expr": "1",
            "legendFormat": "Failed",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                }
              ]
            },
            "unit": "short"
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "Failed"
              },
              "properties": [
                {
                  "id": "color",
                  "value": {
                    "fixedColor": "red",
                    "mode": "fixed"
                  }
                },
                {
                  "id": "links",
                  "value": [
                    {
                      "title": "View Failed Run Details",
                      "url": "https://github.com/almightymoon/Pipeline/actions",
                      "targetBlank": true
                    }
                  ]
                }
              ]
            }
          ]
        }
      },
      {
        "id": 2,
        "title": "Test Results",
        "type": "stat",
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "targets": [
          {
            "expr": "41",
            "legendFormat": "Tests Passed",
            "refId": "A"
          },
          {
            "expr": "1",
            "legendFormat": "Tests Failed",
            "refId": "B"
          },
          {
            "expr": "87.5",
            "legendFormat": "Coverage %",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                }
              ]
            },
            "unit": "short"
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "Tests Failed"
              },
              "properties": [
                {
                  "id": "color",
                  "value": {
                    "fixedColor": "red",
                    "mode": "fixed"
                  }
                },
                {
                  "id": "links",
                  "value": [
                    {
                      "title": "View Test Failure Details",
                      "url": "https://github.com/almightymoon/Pipeline/actions",
                      "targetBlank": true
                    }
                  ]
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "Coverage %"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "percent"
                },
                {
                  "id": "color",
                  "value": {
                    "fixedColor": "blue",
                    "mode": "fixed"
                  }
                }
              ]
            }
          ]
        }
      },
      {
        "id": 3,
        "title": "Code Quality Metrics",
        "type": "stat",
        "gridPos": {
          "h": 6,
          "w": 24,
          "x": 0,
          "y": 6
        },
        "targets": [
          {
            "expr": "407",
            "legendFormat": "TODO Comments",
            "refId": "A"
          },
          {
            "expr": "770",
            "legendFormat": "Debug Statements",
            "refId": "B"
          },
          {
            "expr": "15",
            "legendFormat": "Large Files",
            "refId": "C"
          },
          {
            "expr": "1192",
            "legendFormat": "Total Issues",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 100
                },
                {
                  "color": "red",
                  "value": 500
                }
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 4,
        "title": "Security Scan Results",
        "type": "stat",
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 0,
          "y": 12
        },
        "targets": [
          {
            "expr": "1",
            "legendFormat": "Vulnerabilities",
            "refId": "A"
          },
          {
            "expr": "0",
            "legendFormat": "Secrets Found",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 1
                },
                {
                  "color": "red",
                  "value": 5
                }
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 5,
        "title": "Repository Information",
        "type": "text",
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 12,
          "y": 12
        },
        "options": {
          "mode": "markdown",
          "content": "## Repository Details\n\n**Name:** tensorflow-models  \n**URL:** https://github.com/tensorflow/models  \n**Branch:** master  \n**Scan Type:** full  \n\n## Priority Actions\n- **HIGH:** Remove 770 debug statements\n- **HIGH:** Address 407 TODO comments\n- **MEDIUM:** Optimize 15 large files\n\n---\n**Last Updated:** $(date)"
        }
      },
      {
        "id": 6,
        "title": "Failed Run Details",
        "type": "logs",
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 18
        },
        "targets": [
          {
            "expr": "{job=\"pipeline\"} |= \"ERROR\" or |= \"FAILED\" or |= \"error\" or |= \"failed\"",
            "refId": "A"
          }
        ],
        "options": {
          "showTime": true,
          "showLabels": true,
          "wrapLogMessage": true,
          "prettifyLogMessage": true,
          "enableLogDetails": true,
          "sortOrder": "Descending"
        }
      },
      {
        "id": 7,
        "title": "Recent Pipeline Runs",
        "type": "table",
        "gridPos": {
          "h": 6,
          "w": 24,
          "x": 0,
          "y": 26
        },
        "targets": [
          {
            "expr": "label_join(pipeline_runs_total, \"run_id\", \",\", \"run_id\")",
            "format": "table",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "displayMode": "color-background"
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "status"
              },
              "properties": [
                {
                  "id": "mappings",
                  "value": [
                    {
                      "type": "value",
                      "value": "success",
                      "text": "SUCCESS",
                      "color": "green"
                    },
                    {
                      "type": "value",
                      "value": "failure",
                      "text": "FAILED",
                      "color": "red"
                    },
                    {
                      "type": "value",
                      "value": "running",
                      "text": "RUNNING",
                      "color": "yellow"
                    }
                  ]
                }
              ]
            }
          ]
        },
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true
              },
              "indexByName": {
                "run_id": 0,
                "status": 1,
                "Value": 2
              },
              "renameByName": {
                "run_id": "Run ID",
                "status": "Status",
                "Value": "Count"
              }
            }
          }
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s"
  },
  "overwrite": true
}
