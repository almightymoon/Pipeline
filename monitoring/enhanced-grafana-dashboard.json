{
  "dashboard": {
    "id": null,
    "uid": "pipeline-metrics-logs",
    "title": "ðŸš€ Enterprise Pipeline - Metrics & Logs",
    "tags": ["pipeline", "metrics", "logs", "security", "quality"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "ðŸ“Š Pipeline Execution Overview",
        "type": "stat",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "github_actions_workflow_run_duration_seconds{workflow=\"Working ML Pipeline\"}",
            "legendFormat": "Pipeline Duration (s)",
            "refId": "A"
          },
          {
            "expr": "count(github_actions_workflow_run_conclusion{conclusion=\"success\"})",
            "legendFormat": "Successful Runs",
            "refId": "B"
          },
          {
            "expr": "count(github_actions_workflow_run_conclusion{conclusion=\"failure\"})",
            "legendFormat": "Failed Runs",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 300},
                {"color": "red", "value": 600}
              ]
            },
            "unit": "s"
          }
        }
      },
      {
        "id": 2,
        "title": "ðŸ”’ Security Scan Results",
        "type": "table",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 0},
        "targets": [
          {
            "expr": "trivy_vulnerabilities_total",
            "legendFormat": "{{severity}} - {{target}}",
            "refId": "A",
            "format": "table"
          }
        ],
        "fieldConfig": {
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "severity"},
              "properties": [
                {
                  "id": "custom.displayMode",
                  "value": "color-background"
                },
                {
                  "id": "mappings",
                  "value": [
                    {"type": "value", "value": "CRITICAL", "text": "ðŸ”´ CRITICAL", "color": "red"},
                    {"type": "value", "value": "HIGH", "text": "ðŸŸ  HIGH", "color": "orange"},
                    {"type": "value", "value": "MEDIUM", "text": "ðŸŸ¡ MEDIUM", "color": "yellow"},
                    {"type": "value", "value": "LOW", "text": "ðŸŸ¢ LOW", "color": "green"}
                  ]
                }
              ]
            }
          ]
        },
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {"severity": 0, "target": 1, "Value": 2},
              "renameByName": {"Value": "Count"}
            }
          }
        ]
      },
      {
        "id": 3,
        "title": "ðŸ“ˆ Test Results Trend",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 0},
        "targets": [
          {
            "expr": "rate(pytest_tests_passed_total[5m])",
            "legendFormat": "âœ… Passed",
            "refId": "A"
          },
          {
            "expr": "rate(pytest_tests_failed_total[5m])",
            "legendFormat": "âŒ Failed",
            "refId": "B"
          },
          {
            "expr": "rate(pytest_tests_skipped_total[5m])",
            "legendFormat": "â­ï¸ Skipped",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10,
              "showPoints": "auto"
            },
            "color": {"mode": "palette-classic"}
          },
          "overrides": [
            {"matcher": {"id": "byName", "options": "âœ… Passed"}, "properties": [{"id": "color", "value": {"fixedColor": "green", "mode": "fixed"}}]},
            {"matcher": {"id": "byName", "options": "âŒ Failed"}, "properties": [{"id": "color", "value": {"fixedColor": "red", "mode": "fixed"}}]}
          ]
        }
      },
      {
        "id": 4,
        "title": "ðŸ“ Pipeline Execution Logs",
        "type": "logs",
        "gridPos": {"h": 12, "w": 24, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "{job=\"github-actions\"} |= \"Pipeline\" | json | line_format \"{{.timestamp}} [{{.level}}] {{.message}}\"",
            "refId": "A"
          }
        ],
        "options": {
          "showTime": true,
          "showLabels": true,
          "showCommonLabels": false,
          "wrapLogMessage": true,
          "prettifyLogMessage": true,
          "enableLogDetails": true,
          "dedupStrategy": "none",
          "sortOrder": "Descending"
        }
      },
      {
        "id": 5,
        "title": "ðŸ› Error Logs",
        "type": "logs",
        "gridPos": {"h": 10, "w": 12, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "{job=\"github-actions\"} |~ \"(?i)(error|fail|exception)\" | json | line_format \"{{.timestamp}} âŒ {{.message}}\"",
            "refId": "A"
          }
        ],
        "options": {
          "showTime": true,
          "showLabels": false,
          "wrapLogMessage": true,
          "prettifyLogMessage": true,
          "enableLogDetails": true,
          "sortOrder": "Descending"
        },
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "fixed", "fixedColor": "red"}
          }
        }
      },
      {
        "id": 6,
        "title": "âœ… Success Logs",
        "type": "logs",
        "gridPos": {"h": 10, "w": 12, "x": 12, "y": 20},
        "targets": [
          {
            "expr": "{job=\"github-actions\"} |~ \"(?i)(success|complete|passed)\" | json | line_format \"{{.timestamp}} âœ… {{.message}}\"",
            "refId": "A"
          }
        ],
        "options": {
          "showTime": true,
          "showLabels": false,
          "wrapLogMessage": true,
          "prettifyLogMessage": true,
          "enableLogDetails": true,
          "sortOrder": "Descending"
        },
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "fixed", "fixedColor": "green"}
          }
        }
      },
      {
        "id": 7,
        "title": "ðŸ“Š Code Quality Metrics",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 30},
        "targets": [
          {
            "expr": "code_quality_score",
            "legendFormat": "Quality Score",
            "refId": "A"
          },
          {
            "expr": "code_coverage_percentage",
            "legendFormat": "Test Coverage %",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 50},
                {"color": "green", "value": 80}
              ]
            }
          }
        }
      },
      {
        "id": 8,
        "title": "ðŸ” Scan Metrics Summary",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 30},
        "targets": [
          {
            "expr": "sum(external_repo_files_scanned_total)",
            "legendFormat": "Files Scanned",
            "refId": "A"
          },
          {
            "expr": "sum(external_repo_lines_analyzed_total)",
            "legendFormat": "Lines Analyzed",
            "refId": "B"
          },
          {
            "expr": "sum(external_repo_scan_duration_seconds)",
            "legendFormat": "Scan Duration (s)",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "displayMode": "gradient",
              "orientation": "horizontal"
            }
          }
        }
      },
      {
        "id": 9,
        "title": "ðŸš¨ Security Issues Timeline",
        "type": "bargauge",
        "gridPos": {"h": 10, "w": 24, "x": 0, "y": 38},
        "targets": [
          {
            "expr": "sum by (severity) (trivy_vulnerabilities_found)",
            "legendFormat": "{{severity}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 5},
                {"color": "orange", "value": 10},
                {"color": "red", "value": 20}
              ]
            },
            "custom": {
              "displayMode": "gradient",
              "orientation": "horizontal"
            }
          }
        },
        "options": {
          "displayMode": "gradient",
          "orientation": "horizontal",
          "showUnfilled": true
        }
      },
      {
        "id": 10,
        "title": "ðŸ“‹ Jira Integration Status",
        "type": "table",
        "gridPos": {"h": 10, "w": 24, "x": 0, "y": 48},
        "targets": [
          {
            "expr": "jira_issues_created_total",
            "legendFormat": "{{project}} - {{issue_type}}",
            "refId": "A",
            "format": "table"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true},
              "indexByName": {},
              "renameByName": {
                "project": "Project",
                "issue_type": "Issue Type",
                "Value": "Count"
              }
            }
          }
        ]
      },
      {
        "id": 11,
        "title": "â±ï¸ Pipeline Stage Duration",
        "type": "timeseries",
        "gridPos": {"h": 10, "w": 24, "x": 0, "y": 58},
        "targets": [
          {
            "expr": "github_actions_job_duration_seconds{job=\"Build & Push Image\"}",
            "legendFormat": "ðŸ”¨ Build",
            "refId": "A"
          },
          {
            "expr": "github_actions_job_duration_seconds{job=\"Run Tests\"}",
            "legendFormat": "ðŸ§ª Tests",
            "refId": "B"
          },
          {
            "expr": "github_actions_job_duration_seconds{job=\"Security Analysis\"}",
            "legendFormat": "ðŸ”’ Security",
            "refId": "C"
          },
          {
            "expr": "github_actions_job_duration_seconds{job=\"Deploy to Kubernetes\"}",
            "legendFormat": "ðŸš€ Deploy",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "bars",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "fillOpacity": 80,
              "showPoints": "never",
              "stacking": {"mode": "normal", "group": "A"}
            },
            "color": {"mode": "palette-classic"},
            "unit": "s"
          }
        }
      },
      {
        "id": 12,
        "title": "ðŸ“Š Real-Time Metrics Stream",
        "type": "logs",
        "gridPos": {"h": 12, "w": 24, "x": 0, "y": 68},
        "targets": [
          {
            "expr": "{job=\"prometheus-pushgateway\"} | json | line_format \"[{{.job}}] {{.metric}}: {{.value}}\"",
            "refId": "A"
          }
        ],
        "options": {
          "showTime": true,
          "showLabels": true,
          "wrapLogMessage": false,
          "prettifyLogMessage": true,
          "enableLogDetails": true,
          "dedupStrategy": "none",
          "sortOrder": "Descending"
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "workflow",
          "type": "query",
          "label": "Workflow",
          "query": "label_values(github_actions_workflow_run_duration_seconds, workflow)",
          "refresh": 1,
          "multi": false,
          "includeAll": false
        },
        {
          "name": "branch",
          "type": "query",
          "label": "Branch",
          "query": "label_values(github_actions_workflow_run_duration_seconds, branch)",
          "refresh": 1,
          "multi": false,
          "includeAll": true
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "expr": "changes(github_actions_workflow_run_number{workflow=\"Working ML Pipeline\"}[5m]) > 0",
          "tagKeys": "workflow,branch",
          "textFormat": "Pipeline #{{run_number}} deployed",
          "iconColor": "green"
        },
        {
          "name": "Failures",
          "datasource": "Prometheus",
          "expr": "github_actions_workflow_run_conclusion{conclusion=\"failure\"}",
          "tagKeys": "workflow,branch",
          "textFormat": "Pipeline failed: {{workflow}}",
          "iconColor": "red"
        }
      ]
    }
  },
  "overwrite": true,
  "folderId": 0,
  "folderUid": "ml-pipeline-reports"
}
